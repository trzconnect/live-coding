{
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "start": "concurrently \"turbo run start --no-cache\" \"yarn watch\"",
    "watch": "turbo run watch --filter=kit-ui",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "test": "turbo run test --filter=algo --"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.5.1",
    "turbo": "latest",
    "concurrently": "^8.2.0"
  },
  "name": "live-coding",
  "packageManager": "yarn@1.22.19"
}
