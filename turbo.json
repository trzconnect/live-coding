{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      "outputs": ["build/**"],
      "dependsOn": ["^build"]
    },
    "lint": {},
    "start": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^kit-ui#build"]
    },
    "test": {
      "cache": false,
      "persistent": true
    },
    "watch": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^kit-ui#build"]
    }
  }
}
